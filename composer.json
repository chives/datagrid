{
    "name": "fsi/datagrid",
    "type": "library",
    "description": "FSi DataGrid Component",
    "keywords": ["fsi", "component", "datagrid"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/event-dispatcher" : "^2.2|^3.0",
        "symfony/property-access": "^2.3|^3.0",
        "symfony/options-resolver": "^2.6|^3.0",
        "fsi/reflection" : "0.9.*",
        "fsi/data-indexer" : "0.9.*"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "doctrine/common": "^2.5",
        "gedmo/doctrine-extensions": "^2.3.10",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Component\\DataGrid\\": "lib/",
            "FSi\\Component\\DataGrid\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
